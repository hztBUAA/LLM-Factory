# OpenAI Configuration (Azure OpenAI)
OPENAI_API_KEY=your_azure_openai_api_key
OPENAI_API_BASE=https://your-resource.openai.azure.com/
OPENAI_API_VERSION=2024-02-01
OPENAI_MODEL=gpt-4o

# Qwen Configuration
QWEN_API_KEY=your_qwen_api_key
QWEN_API_BASE=https://dashscope.aliyuncs.com/api/v1
QWEN_MODEL=qwen-turbo

# DeepSeek Configuration
DEEPSEEK_API_KEY=your_deepseek_api_key
DEEPSEEK_API_BASE=https://api.deepseek.com/v1
DEEPSEEK_MODEL=deepseek-chat

# Claude Configuration (AWS Bedrock)
CLAUDE_ACCESS_KEY=your_aws_access_key_id
CLAUDE_SECRET_KEY=your_aws_secret_access_key
CLAUDE_REGION=us-east-1
CLAUDE_MODEL=anthropic.claude-3-5-sonnet-20241022-v2:0

# Gemini Configuration
GEMINI_API_KEY=your_gemini_api_key
GEMINI_PROJECT_ID=your_gcp_project_id
GEMINI_REGION=us-central1
GEMINI_MODEL=gemini-2.0-flash-exp

# Proxy Configuration (optional)
HTTP_PROXY=http://proxy.example.com:8080
HTTPS_PROXY=http://proxy.example.com:8080

# Provider-specific proxy (optional)
OPENAI_HTTP_PROXY=http://openai-proxy.example.com:8080
QWEN_HTTP_PROXY=http://qwen-proxy.example.com:8080
